{
  "connection": "default",
  "collectionName": "gameposts",
  "info": {
    "name": "gamepost",
    "description": "game descripton"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "gamecategories": {
      "collection": "gamecategory",
      "via": "gameposts"
    },
    "Picture": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}